    <body>
      <trans-unit approved="no" id="1" xml:space="preserve">
      <source xml:lang="ru">Назначение программы</source>
   </trans-unit>
      <trans-unit approved="no" id="2" xml:space="preserve">
      <source xml:lang="ru">Программное обеспечение «<ph ctype="x-other" id="0">&lt;ph keyref="software_name" status="removeContent" class="- topic/ph "&gt;</ph>
